{
  "name": "juntina",
  "version": "0.1.0",
  "description": "Chatbot JUNTINA",
  "main": "./dist/app.js",
  "scripts": {
    "test": "node ./dist/test.js",
    "build": "tsc",
    "seed-mongo": "node ./dist/infrastructure/seeds/module.seed.js",
    "build:watch": "tsc --watch",
    "dev": "nodemon ./dist/app.js",
    "ngrok": "ngrok http 8088",
    "start": "tsc && node ./dist/app.js",
    "migrate": "npx sequelize db:migrate",
    "migrate:generate": "npx sequelize migration:generate --name",
    "migrate:undo": "npx sequelize db:migrate:undo",
    "migrate:undo:all": "npx sequelize db:migrate:undo:all",
    "migrate:restart:all": "yarn migrate:undo && yarn migrate && yarn run seed",
    "seed": "npx sequelize db:seed:all --debug",
    "seed:restart:all": "yarn run seed:undo:all && npx sequelize db:seed:all --debug",
    "seed:generate": "npx sequelize seed:generate --name",
    "seed:undo:all": "npx sequelize db:seed:undo:all",
    "docker": " ",
    "lint": "eslint . --ext .ts",
    "lint:watch": "eslint . --ext .ts",
    "format": "prettier --write \"app/**/*.ts\"",
    "format:watch": "npx onchange \"app/**/*.ts\" -- npx prettier --write --ignore-unknown {{changed}}",
    "tunnel": "lt --port 8088 --subdomain chatbot-juntina"
  },
  "author": "Hendry M. Flores",
  "license": "ISC",
  "dependencies": {
    "@types/convert-array-to-csv": "^2.0.0",
    "@types/node-cron": "^3.0.8",
    "@types/sequelize": "^4.28.15",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "convert-array-to-csv": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "localtunnel": "^2.0.2",
    "mongoose": "^7.4.0",
    "mysql2": "^3.4.0",
    "node-cron": "^3.0.2",
    "passport": "^0.6.0",
    "sequelize": "^6.32.1",
    "whiz-logger": "^1.3.6"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "onchange": "^7.1.0",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.6.1",
    "typescript": "^5.1.3"
  }
}
