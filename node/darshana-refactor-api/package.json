{
  "name": "darshana-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "tsc": "tsc",
    "dev-compile": "tsc && node server.js",
    "dev-2": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run dev-compile\"",
    "dev": "nodemon server.js",
    "start": "node server.js",
    "nodemon": "nodemon server.js",
    "migration:orm:up": "npx sequelize-cli db:migrate",
    "migration:orm:generate": "npx sequelize-cli migration:generate --name",
    "migration:orm:rollback": "npx sequelize-cli db:migrate:undo",
    "migration:orm:delete": "npx sequelize-cli db:migrate:undo:all",
    "migration:orm:seeds:up": "npx sequelize-cli db:seed:all",
    "migration:orm:seeds:undo:all": "npx sequelize-cli db:seed:undo:all --seeders-path ./database/seeders-sequelize",
    "migration:orm:seeds:generate": "npx sequelize-cli seed:generate --seeders-path ./database/seeders-sequelize --name",
    "migration:init": "migrate init --migrations-dir ./database/migrations",
    "migration:create": "migrate create --migrations-dir ./database/migrations --template-file ./database/migrations/.template.js",
    "migration:list": "migrate list --migrations-dir ./database/migrations",
    "migration:up": "migrate up --migrations-dir ./database/migrations --env ./.env",
    "migration:down": "migrate down --migrations-dir ./database/migrations --env ./.env",
    "migration:seed": "node ./database/seeders/.seeds.js ./database/seeders/seeder.config.js",
    "lint": "eslint \"app/**/*.js\" --cache --fix",
    "format": "prettier . --write",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/whiz.pe/darshana-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/whiz.pe/darshana-api/issues"
  },
  "homepage": "https://gitlab.com/whiz.pe/darshana-api#readme",
  "dependencies": {
    "@paypal/checkout-server-sdk": "^1.0.2",
    "@paypal/payouts-sdk": "^1.0.0",
    "algosdk": "^1.12.0",
    "aobj": "^1.0.7",
    "aws-sdk": "^2.946.0",
    "body-parser": "^1.20.0",
    "cheerio": "^1.0.0-rc.3",
    "cli-progress": "^3.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "ejs": "^3.1.5",
    "express": "^4.18.1",
    "fast-sort": "^2.1.1",
    "form-data": "^3.0.0",
    "google-auth-library": "^6.1.3",
    "has-keys": "^0.1.0",
    "helmet": "^3.21.3",
    "iconv-lite": "latest",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.25.0",
    "mercadopago": "^1.3.1",
    "migrate": "^1.6.2",
    "moment": "^2.29.4",
    "multer": "^1.4.4",
    "multer-s3": "^2.9.0",
    "mysql2": "^3.1.2",
    "mysqlm": "^2.0.6",
    "nanoid": "^3.1.21",
    "nanoid-dictionary": "^4.2.0",
    "nanoid-generate": "^1.0.0",
    "nested-property": "^2.0.0",
    "node-cron": "^3.0.2",
    "node-fetch": "^2.6.7",
    "openai": "^3.2.1",
    "paypal-rest-sdk": "^1.8.1",
    "pug": "^3.0.0",
    "qs": "^6.9.4",
    "sanitize-html": "^1.23.0",
    "sequelize": "^6.29.0",
    "short-uuid": "^4.1.0",
    "shortid": "^2.2.15",
    "slugify": "^1.4.7",
    "socket.io": "^3.0.1",
    "stripe": "^11.18.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "twilio": "^3.42.2",
    "uuid": "^8.3.2",
    "validatorjs": "3.22.1",
    "whiz-logger": "^1.3.6",
    "whiz-sdk-node": "^1.5.5",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.1.4",
    "cli-progress": "^3.7.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.8.4",
    "sequelize-cli": "^6.6.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "eslint --cache --fix"
    ],
    "*.{js,css,md}": "npm run format",
    "*.--write": "prettier --write"
  }
}
